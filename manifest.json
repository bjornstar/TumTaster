{
	"name": "TumTaster",
	"version": "0.5.0",
	"description": "An extension that creates download links for the MP3s you see on Tumblr.",
	"background": {
		"page": "index.html"
	},
	"browser_action": {
		"default_icon": "data/Icon-16.png",
		"default_popup": "data/popup.html",
		"default_title": "TumTaster"
	},
	"content_scripts": [ {
		"js": [ "data/script.js" ],
		"matches": [ "http://*/*", "https://*/*" ]
	} ],
	"icons": {
		"16": "data/Icon-16.png",
		"32": "data/Icon-32.png",
		"48": "data/Icon-48.png",
		"64": "data/Icon-64.png",
		"128": "data/Icon-128.png"
	},
	"manifest_version": 2,
	"options_page": "data/options.html",
	"permissions": [
		"http://*/*"
	]
}